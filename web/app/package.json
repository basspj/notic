{
  "name": "@web/app",
  "version": "0.0.1",
  "description": "React and more",
  "scripts": {
    "clean-coverage": "rimraf 'coverage'",
    "pretest": "yarn clean-coverage",
    "test": "jest -c jest.json --no-cache",
    "dev": "ts-node -r tsconfig-paths/register --project scripts/tsconfig.json scripts/dev.ts",
    "build": "ts-node -r tsconfig-paths/register --project scripts/tsconfig.json scripts/build.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "e2e": "ts-node -r tsconfig-paths/register --project scripts/tsconfig.json scripts/e2e.ts"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "styled-components": "3.1.6"
  },
  "devDependencies": {
    "@storybook/react": "3.3.14",
    "typescript": "2.7.2",
    "ts-node": "5.0.0",
    "tsconfig-paths": "3.1.1",
    "jest": "22.4.0",
    "babel-jest": "22.4.0",
    "ts-jest": "22.0.4",
    "puppeteer": "1.0.0",
    "webpack": "3.11.0",
    "webpack-dev-middleware": "2.0.5",
    "webpack-dev-server": "2.11.1",
    "react-dev-utils": "5.0.0",
    "promise": "8.0.1",
    "object-assign": "4.1.1",
    "autoprefixer": "8.0.0",
    "html-webpack-plugin": "2.30.1",
    "extract-text-webpack-plugin": "3.0.2",
    "webpack-manifest-plugin": "1.3.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "postcss-flexbugs-fixes": "3.3.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "fs-extra": "5.0.0",
    "dotenv": "5.0.0",
    "rimraf": "2.6.2",
    "chalk": "2.3.1",
    "cross-env": "5.1.3"
  },
  "license": "MIT",
  "homepage": "https://github.com/basspj/notic#readme"
}
