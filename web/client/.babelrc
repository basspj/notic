{
  "presets": ["es2015", "react", "stage-2"],
  "sourceMaps": "inline",
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "~": "."
        }
      }
    ]
  ],
  "env": {
    "web": {
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["."],
            "alias": {
              "~": "./web/client"
            }
          }
        ]
      ]
    }
  }
}
