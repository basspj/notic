{
  "name": "notic-mobile",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npm run bootstrap",
    "bootstrap": "lerna bootstrap",
    "bootstrap-hoist": "npm run bootstrap -- --hoist",
    "publish": "npm run clean-build && npm run build && lerna publish --silent",
    "clean": "rimraf $TMPDIR/react-* && npm cache clean -f",
    "clean-watch": "npm run clean && watchman watch-del-all",
    "clean-coverage": "rimraf 'coverage'",
    "clean-node_modules": "rimraf 'node_modules' 'packages/*/node_modules'",
    "clean-package-lock":
      "rimraf 'package-lock.json' 'packages/*/package-lock.json'",
    "clean-dist": "rimraf 'dist' 'packages/*/dist'",
    "clean-all":
      "npm run clean-dist && npm run clean-package-lock && npm run clean-node_modules",
    "dedupe": "npm dedupe",
    "tslint": "tslint -c tslint.json '**/*.ts{,x}'",
    "tslint-type": "npm run lint -- --type-check",
    "eslint": "eslint -c .eslintrc '**/*.ts{,x}'",
    "lint": "npm run tslint && npm run eslint",
    "prettier": "npm run prettier-typescript && npm run prettier-json",
    "prettier-typescript":
      "prettier --config .prettierrc --parser typescript --write '{packages/**,scripts}/**/*.ts{,x}'",
    "prettier-json":
      "prettier --config .prettierrc --parser json --write '{*.json,.*rc}'",
    "test": "jest --config jest.json",
    "posttest": "codecov -f coverage/*.json",
    "test-clean": "npm run test -- --no-cache",
    "test-watch": "npm run test -- --watch",
    "snapshot": "npm run test -- --updateSnapshot",
    "e2e-build": "detox build -c ios.sim.debug",
    "e2e-test": "detox test -c ios.sim.debug",
    "storybook": "storybook start -p 7007",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "git-add": "git add .",
    "cz": "git-cz",
    "start": "react-native start",
    "start-cache": "react-native start -- --reset-cache",
    "link": "react-native link",
    "prebuild": "rimraf build",
    "build": "tsc",
    "postbuild": "npm run clean-build",
    "postversion":
      "node node_modules/react-native-version/cli.js --never-amend",
    "android": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "android-build": "cd android && ./gradlew assembleRelease --stacktrace",
    "android-install":
      "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android-devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android-shake":
      "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "android-log": "react-native log-android",
    "android-clean": "rimraf android/app/build && rimraf android/build",
    "ios": "react-native run-ios",
    "ios-log": "react-native log-ios",
    "ios-clean": "rimraf ios/build",
    "native-clean": "npm run android-clean && npm run ios-clean",
    "bundle-clean-android":
      "rimraf index.android.bundle.js index.android.bundle.js.meta",
    "bundle-clean-ios": "rimraf main.jsbundle.js main.jsbundle.js.meta",
    "obfuscate-android":
      "javascript-obfuscator index.android.bundle.js --output bundle/android/app/src/main/assets/index.android.bundle --compact true --selfDefending true",
    "obfuscate-ios":
      "javascript-obfuscator main.jsbundle.js --output bundle/ios/main.jsbundle --compact true --selfDefending true",
    "postobfuscate-android": "npm run bundle-clean-android",
    "postobfuscate-ios": "npm run bundle-clean-ios",
    "prebundle-android": "rimraf bundle/android",
    "prebundle-ios": "rimraf bundle/ios",
    "bundle-android":
      "react-native bundle --platform android --dev false --entry-file index.js --bundle-output index.android.bundle.js --assets-dest bundle/android/app/src/main/res/",
    "bundle-ios":
      "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output main.jsbundle.js --assets-dest bundle/ios",
    "prebundle": "rimraf bundle",
    "bundle": "npm run bundle-android && npm run bundle-ios",
    "postbundle-android": "npm run obfuscate-android",
    "postbundle-ios": "npm run obfuscate-ios"
  },
  "dependencies": {
    "react": "16.0.0-beta.5",
    "react-native": "0.50.1"
  },
  "devDependencies": {
    "@types/react": "16.0.20",
    "@types/react-native": "0.50.0",
    "@types/jest": "21.1.5",
    "@types/enzyme": "3.1.3",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__react": "3.0.5",
    "@commitlint/cli": "4.2.2",
    "@commitlint/config-angular": "4.2.1",
    "@commitlint/config-lerna-scopes": "4.2.1",
    "@storybook/react-native": "3.2.14",
    "@storybook/addon-actions": "3.2.14",
    "@storybook/addon-links": "3.2.14",
    "babel-preset-react-native": "4.0.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-jest": "21.2.0",
    "babel-eslint": "8.0.1",
    "react": "16.0.0-beta.5",
    "react-dom": "16.0.0-beta.5",
    "react-test-renderer": "16.0.0-beta.5",
    "react-native-typescript-transformer": "1.1.4",
    "lerna": "2.5.1",
    "typescript": "2.6.1",
    "tsutils": "2.12.2",
    "tslib": "1.8.0",
    "ts-node": "3.3.0",
    "ts-jest": "21.1.4",
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "tslint-config-airbnb": "5.3.0",
    "typedoc": "0.9.0",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.4",
    "jest": "21.2.1",
    "jest-react-native": "18.0.0",
    "jest-enzyme": "4.0.1",
    "codecov": "3.0.0",
    "sinon": "4.1.1",
    "detox": "5.9.3",
    "prepack": "0.2.10",
    "javascript-obfuscator": "0.12.1",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "husky": "0.14.3",
    "lint-staged": "4.3.0",
    "prettier": "1.7.4",
    "prettier-eslint": "8.2.1",
    "eslint": "4.10.0",
    "eslint-plugin-typescript": "0.8.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-wix": "1.1.16",
    "typescript-eslint-parser": "8.0.1",
    "rimraf": "2.6.2"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "useBabelrc": true
      }
    },
    "transform": {
      ".+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      ".+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/tests/.*.spec.(js|ts|tsx)?$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"],
    "moduleDirectories": ["node_modules", "src"],
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1",
      "~tests/(.*)": "<rootDir>/tests/$1",
      "~e2e/(.*)": "<rootDir>/e2e/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/e2e/init.ts",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": ["/node_modules/", "/tests/", "/scripts/"]
  },
  "detox": {
    "runner": "jest",
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/app-debug.apk",
        "build":
          "pushd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/app-release.apk",
        "build":
          "pushd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && popd",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      },
      "ios.sim.debug": {
        "binaryPath":
          "ios/build/Build/Products/Debug-iphonesimulator/notic.app",
        "build":
          "xcodebuild -project ios/notic.xcodeproj -scheme notic -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 6"
      },
      "ios.sim.release": {
        "binaryPath":
          "ios/build/Build/Products/Release-iphonesimulator/notic.app",
        "build":
          "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -project ios/notic.xcodeproj -scheme notic -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 6"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "description": "React Native and more",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basspj/notic-mobile.git"
  },
  "author": "basspj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/basspj/notic-mobile/issues"
  },
  "homepage": "https://github.com/basspj/notic-mobile#readme"
}
