{
  "name": "notic",
  "version": "0.0.1",
  "description": "React, React Native and more",
  "private": true,
  "scripts": {
    "script-postinstall": "ts-node --project scripts/postinstall ./scripts/postinstall",
    "postinstall": "yarn script-postinstall && yarn bootstrap",
    "prebootstrap": "yarn clean-dist",
    "bootstrap": "lerna bootstrap",
    "pub": "lerna publish --silent --skip-npm --conventional-commits",
    "pub-yes": "yarn pub -- --yes",
    "push": "git push --follow-tags",
    "clean": "rimraf $TMPDIR/react-* && yarn cache clean && watchman watch-del-all",
    "clean-lock": "rimraf 'yarn.lock' '{mobile,web}/*/yarn.lock'",
    "clean-coverage": "rimraf 'coverage'",
    "clean-dist": "rimraf '{mobile,web}/*/dist'",
    "clean-packages": "rimraf '{mobile,web}/*/{dist,node_modules}'",
    "tslint": "tslint -c tslint.json '**/*.ts{,x}'",
    "tslint-type": "yarn lint --type-check",
    "eslint": "eslint -c .eslintrc '**/*.ts{,x}'",
    "stylelint": "stylelint -c .stylelintrc '**/*.ts{,x}'",
    "lint": "yarn tslint && yarn eslint && yarn stylelint",
    "pretest": "yarn clean-coverage",
    "test": "yarn test-mobile && yarn test-web",
    "test-mobile": "BABEL_ENV=mobile jest -c jest.mobile.json --no-cache",
    "test-web": "BABEL_ENV=web jest -c jest.web.json --no-cache",
    "posttest": "yarn coverage-merge",
    "coverage-merge":
      "istanbul-merge --out coverage/coverage-merged.json coverage/mobile/coverage-final.json coverage/web/coverage-final.json",
    "postcoverage-merge": "rimraf 'coverage/{mobile,web}' && yarn coverage-report",
    "coverage-report": "codecov -f coverage/*.json",
    "snapshot": "yarn test --updateSnapshot",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "cz": "git-cz",
    "cz-retry": "git-cz --retry"
  },
  "devDependencies": {
    "@types/react": "16.0.25",
    "@types/react-dom": "16.0.3",
    "@types/react-hot-loader": "3.0.5",
    "@types/react-native": "0.50.7",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__react": "3.0.5",
    "@types/lodash": "4.14.86",
    "@types/jest": "21.1.8",
    "@types/enzyme": "3.1.5",
    "@types/sinon": "4.0.0",
    "@types/color": "2.0.0",
    "@types/shelljs": "0.7.6",
    "@types/puppeteer": "0.13.6",
    "@types/webpack": "3.8.1",
    "@types/webpack-dev-server": "2.9.2",
    "@types/fs-extra": "4.0.5",
    "@commitlint/cli": "5.1.1",
    "@commitlint/config-conventional": "5.1.3",
    "babel-jest": "21.2.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-eslint": "8.0.2",
    "react": "16.2.0",
    "react-native": "0.50.4",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "lerna": "2.5.1",
    "typescript": "2.6.2",
    "tsutils": "2.13.0",
    "ts-node": "3.3.0",
    "tsconfig-paths": "2.4.0",
    "ts-jest": "21.2.3",
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "tslint-config-airbnb": "5.4.2",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.1",
    "istanbul-merge": "1.1.1",
    "puppeteer": "0.13.0",
    "sinon": "4.1.2",
    "codecov": "3.0.0",
    "commitizen": "2.9.6",
    "cz-lerna-changelog": "1.2.1",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "prettier-eslint-cli": "4.4.0",
    "eslint": "4.12.1",
    "eslint-plugin-typescript": "0.8.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-wix": "1.1.16",
    "typescript-eslint-parser": "9.0.1",
    "stylelint": "8.3.1",
    "stylelint-processor-styled-components": "1.1.1",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-config-standard": "18.0.0",
    "rollup": "0.52.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "lodash": "4.17.4",
    "shelljs": "0.7.8",
    "chalk": "2.3.0",
    "rimraf": "2.6.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-lerna-changelog"
    }
  },
  "repository": "basspj/notic",
  "author": "basspj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basspj/notic/issues"
  },
  "homepage": "https://github.com/basspj/notic#readme"
}
