{
  "name": "notic",
  "private": true,
  "scripts": {
    "install-force": "yarn --skip-integrity-check",
    "types": "ts-node --project scripts/tsconfig.json scripts/types",
    "pretypes-update": "rimraf 'node_modules/@types'",
    "types-update": "yarn install-force",
    "postinstall": "yarn types",
    "boot": "lerna bootstrap",
    "pub": "lerna publish --silent --skip-npm --conventional-commits",
    "pub-yes": "yarn pub -- --yes",
    "push": "git push --follow-tags",
    "updated": "lerna updated",
    "ls": "lerna ls",
    "clean": "lerna clean",
    "clean-lock": "rimraf 'yarn.lock' '{mobile,web}/*/yarn.lock'",
    "clean-coverage": "rimraf 'coverage'",
    "prettier": "yarn prettier-ts && yarn prettier-json",
    "prettier-base": "prettier-eslint --config .prettierrc --eslint-config-path .eslintrc",
    "prettier-ts": "yarn prettier-base -- --parser typescript",
    "prettier-json": "yarn prettier-base -- --parser json",
    "tslint": "tslint -c tslint.json '**/*.ts{,x}'",
    "tslint-type": "yarn tslint --type-check",
    "eslint": "eslint -c .eslintrc '**/*.ts{,x}'",
    "stylelint": "stylelint -c .stylelintrc '**/*.ts{,x}'",
    "lint": "yarn tslint && yarn eslint && yarn stylelint",
    "pretest": "yarn clean-coverage",
    "test": "lerna run test",
    "posttest": "yarn coverage-merge",
    "coverage-merge":
      "istanbul-merge '{mobile,web,shared}/**/coverage/coverage-final.json' --out coverage/coverage-merged.json",
    "postcoverage-merge": "yarn coverage-report",
    "coverage-report": "codecov -f coverage/*.json",
    "snapshot": "yarn test --updateSnapshot",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "cz": "git-cz",
    "cz-retry": "git-cz --retry",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn snyk-protect",
    "react-dev": "yarn react-devtools"
  },
  "dependencies": {
    "snyk": "1.76.0"
  },
  "devDependencies": {
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "@types/react-hot-loader": "3.0.6",
    "@types/react-native": "0.52.12",
    "@types/storybook__addon-actions": "3.0.2",
    "@types/storybook__addon-links": "3.0.3",
    "@types/storybook__react": "3.0.7",
    "@types/lodash": "4.14.104",
    "@types/jest": "22.1.3",
    "@types/enzyme": "3.1.9",
    "@types/sinon": "4.1.3",
    "@types/color": "3.0.0",
    "@types/shelljs": "0.7.8",
    "@types/puppeteer": "1.0.0",
    "@types/webpack": "3.8.8",
    "@types/webpack-dev-server": "2.9.3",
    "@types/fs-extra": "5.0.0",
    "@commitlint/cli": "6.1.0",
    "@commitlint/config-conventional": "6.1.0",
    "@storybook/addon-actions": "3.3.14",
    "@storybook/addon-links": "3.3.14",
    "babel-jest": "22.4.0",
    "babel-polyfill": "6.26.0",
    "babel-plugin-module-resolver": "3.1.0",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-eslint": "8.2.2",
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "awesome-typescript-loader": "3.4.1",
    "babel-loader": "7.1.2",
    "eslint-loader": "1.9.0",
    "source-map-loader": "0.2.3",
    "stylelint-custom-processor-loader": "0.4.0",
    "tslint-loader": "3.5.3",
    "url-loader": "0.6.2",
    "lerna": "2.9.0",
    "typescript": "2.7.2",
    "tsutils": "2.21.1",
    "ts-node": "5.0.0",
    "tsconfig-paths": "3.1.1",
    "ts-jest": "22.0.4",
    "typescript-styled-plugin": "0.6.0",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "tslint-config-airbnb": "5.7.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "22.4.0",
    "jest-enzyme": "4.2.0",
    "istanbul-merge": "1.1.1",
    "puppeteer": "1.1.0",
    "sinon": "4.3.0",
    "codecov": "3.0.0",
    "commitizen": "2.9.6",
    "cz-lerna-changelog": "1.2.1",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "prettier-eslint-cli": "4.7.1",
    "eslint": "4.18.1",
    "eslint-plugin-typescript": "0.8.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-wix": "1.1.16",
    "typescript-eslint-parser": "14.0.0",
    "stylelint": "9.1.0",
    "stylelint-processor-styled-components": "1.2.2",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-config-standard": "18.1.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-typescript2": "0.11.1",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-babel": "3.0.3",
    "lodash": "4.17.5",
    "shelljs": "0.8.1",
    "chalk": "2.3.1",
    "rimraf": "2.6.2",
    "react-devtools": "3.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-lerna-changelog"
