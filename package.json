{
  "name": "notic-mobile",
  "version": "0.0.1",
  "description": "React Native and more",
  "private": true,
  "scripts": {
    "script-postinstall":
      "ts-node --project scripts/postinstall ./scripts/postinstall",
    "postinstall": "npm run script-postinstall && npm run bootstrap",
    "prebootstrap": "npm run clean-dist",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna publish --silent",
    "clean":
      "rimraf $TMPDIR/react-* && npm cache clean -f && watchman watch-del-all",
    "clean-coverage": "rimraf 'coverage'",
    "clean-dist": "rimraf 'packages/*/dist'",
    "clean-node": "rimraf 'packages/*/node_modules'",
    "clean-packages": "rimraf 'packages/*/dist' 'packages/*/node_modules'",
    "tslint": "tslint -c tslint.json '**/*.ts{,x}'",
    "tslint-type": "npm run lint -- --type-check",
    "eslint": "eslint -c .eslintrc '**/*.ts{,x}'",
    "stylelint": "stylelint -c .stylelintrc '**/*.ts{,x}'",
    "lint": "npm run tslint && npm run eslint && npm run stylelint",
    "test": "BABEL_ENV=packages jest -c jest.json --no-cache",
    "test-watch": "npm run test -- --watch",
    "posttest": "codecov -f coverage/*.json",
    "snapshot": "npm run test -- --updateSnapshot",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "git-add": "git add .",
    "cz": "git-cz",
    "cz-retry": "git-cz --retry"
  },
  "devDependencies": {
    "@types/react": "16.0.23",
    "@types/react-native": "0.50.7",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__react": "3.0.5",
    "@types/lodash": "4.14.85",
    "@types/jest": "21.1.6",
    "@types/enzyme": "3.1.4",
    "@types/sinon": "4.0.0",
    "@types/color": "2.0.0",
    "@types/shelljs": "0.7.6",
    "@commitlint/cli": "4.3.0",
    "@commitlint/config-angular": "4.3.0",
    "@commitlint/config-lerna-scopes": "4.3.0",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-eslint": "8.0.2",
    "react": "16.1.1",
    "react-native": "0.50.3",
    "react-dom": "16.1.1",
    "react-test-renderer": "16.1.1",
    "lerna": "2.5.1",
    "typescript": "2.6.1",
    "tsutils": "2.12.2",
    "ts-node": "3.3.0",
    "tsconfig-paths": "2.3.0",
    "ts-jest": "21.2.2",
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "tslint-config-airbnb": "5.4.2",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.1",
    "codecov": "3.0.0",
    "sinon": "4.1.2",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "prettier": "1.8.2",
    "prettier-eslint": "8.2.2",
    "eslint": "4.11.0",
    "eslint-plugin-typescript": "0.8.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-wix": "1.1.16",
    "typescript-eslint-parser": "9.0.0",
    "stylelint": "8.2.0",
    "stylelint-processor-styled-components": "1.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-config-standard": "17.0.0",
    "rollup": "0.51.6",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "lodash": "4.17.4",
    "shelljs": "0.7.8",
    "chalk": "2.3.0",
    "rimraf": "2.6.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "workspaces": ["packages/*"],
  "repository": "basspj/notic-mobile",
  "author": "basspj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basspj/notic-mobile/issues"
  },
  "homepage": "https://github.com/basspj/notic-mobile#readme"
}
