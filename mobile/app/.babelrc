{
  "presets": ["react-native"],
  "sourceMaps": "inline",
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "~": "."
        }
      }
    ]
  ],
  "env": {
    "mobile": {
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["."],
            "alias": {
              "~": "./mobile/app"
            }
          }
        ]
      ]
    }
  }
}
